{
  "name": "importer",
  "version": "2.0.0",
  "description": "Imports data into cougargrades.io",
  "main": "./lib/index.js",
  "bin": {
    "cougarimport": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc -p .",
    "watch": "nodemon --watch src/ --ext js,mjs,json,ts --exec 'clear; npm run build'",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cougargrades/importer.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cougargrades/importer/issues"
  },
  "homepage": "https://github.com/cougargrades/importer#readme",
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^5.0.0",
    "figlet": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^13.9.1",
    "@types/figlet": "^1.2.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
